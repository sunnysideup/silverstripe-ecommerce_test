{
    "name": "sunnysideup/ecommerce_test",
    "type": "silverstripe-module",
    "description": "allows you to review sunnysideup/ecommerce",
    "authors": [
        {
            "name": "Nicolaas Francken",
            "email": "modules@sunnysideup.co.nz",
            "role": "lead developer"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "silverstripe/recipe-cms": "^4.7",

        "silverstripe-themes/simple": "dev-master",

        "sunnysideup/ecommerce": "^12.0",

        "sunnysideup/ecommerce-demo-theme": "dev-master",

        "sunnysideup/ecommerce_advanced_retail_connection": "dev-master",
        "sunnysideup/ecommerce_alsorecommended": "^2.4",
        "sunnysideup/ecommerce_assign_orders": "^1.0",
        "sunnysideup/ecommerce_custom_product_lists": "dev-master",
        "sunnysideup/ecommerce_tax": "^6.0",
        "sunnysideup/ecommerce_discount_coupon": "dev-master",
        "sunnysideup/ecommerce_giftvoucher": "dev-master",
        "sunnysideup/ecommerce_delivery": "^5.6",
        "sunnysideup/ecommerce-google-shopping-feed": "^1.0",
        "sunnysideup/ecommerce_maxmind_minfraud": "^1.1",
        "sunnysideup/ecommerce_orderstep_feedback": "^1.4",
        "sunnysideup/ecommerce_discount_coupon": "^3.12",
        "sunnysideup/ecommerce_security": "^1.4",
        "sunnysideup/ecommerce_second_hand_product": "^1.7",
        "sunnysideup/ecommerce-google-analytics": "^1.0",

        "sunnysideup/payment_directcredit": "^1.2",
        "sunnysideup/payment_dps": "^5.2",
        "sunnysideup/payment_hirepurchase": "^1.0",
        "sunnysideup/payment_instore": "^1.2",

        "sunnysideup/afterpay": "dev-master",

        "sunnysideup/geoip": "dev-master",
        "sunnysideup/google_address_field": "^2.0",
        "sunnysideup/googleanalyticsbasics": "dev-master",

        "sunnysideup/webpack_requirements_backend": "dev-master",
        "sunnysideup/sswebpack_engine_only": "dev-master"
    },
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": []
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "keywords": [
        "Silverstripe-CMS",
        "CMS",
        "Silverstripe",
        "ecommerce",
        "e-commerce",
        "shop",
        "sales",
        "demo"
    ],
    "extra": {
        "installer-name": "ecommerce_test",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "web.config"
        ]
    },
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-4": {
            "Sunnysideup\\EcommerceTest\\": "app/src/"
        },
        "files": [
            "app/src/Page.php",
            "app/src/PageController.php"
        ]
    }
}
