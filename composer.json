{
  "name": "sunnysideup/ecommerce_test",
  "type": "silverstripe-recipe",
  "description": "allows you to review sunnysideup/ecommerce",
  "authors": [
    {
      "name": "Nicolaas Francken",
      "email": "modules@sunnysideup.co.nz",
      "role": "lead developer"
    }
  ],
  "require": {
    "php": ">=8.1",
    "silverstripe/recipe-cms": "^5.0",
    "silverstripe-themes/simple": "dev-master",
    "sunnysideup/ecommerce": "dev-master",
    "sunnysideup/ecommerce-demo-theme": "dev-master",
    "sunnysideup/ecommerce_advanced_retail_connection": "dev-master",
    "sunnysideup/ecommerce_alsorecommended": "dev-master",
    "sunnysideup/ecommerce_assign_orders": "dev-master",
    "sunnysideup/ecommerce_custom_product_lists": "dev-master",
    "sunnysideup/ecommerce_tax": "dev-master",
    "sunnysideup/ecommerce_discount_coupon": "dev-master",
    "sunnysideup/ecommerce_giftvoucher": "dev-master",
    "sunnysideup/ecommerce_delivery": "dev-master",
    "sunnysideup/ecommerce-google-shopping-feed": "dev-master",
    "sunnysideup/ecommerce_maxmind_minfraud": "dev-master",
    "sunnysideup/ecommerce_orderstep_feedback": "dev-master",
    "sunnysideup/ecommerce_quick_coupons": "dev-master",
    "sunnysideup/ecommerce_security": "dev-master",
    "sunnysideup/ecommerce_second_hand_product": "dev-master",
    "sunnysideup/ecommerce-google-analytics": "dev-master",
    "sunnysideup/payment_directcredit": "dev-master",
    "sunnysideup/payment_dps": "dev-master",
    "sunnysideup/payment_hirepurchase": "dev-master",
    "sunnysideup/payment_instore": "dev-master",
    "sunnysideup/afterpay": "dev-master",
    "sunnysideup/geoip": "dev-master",
    "sunnysideup/google_address_field": "dev-master",
    "sunnysideup/googleanalyticsbasics": "dev-master",
    "sunnysideup/webpack_requirements_backend": "dev-master",
    "sunnysideup/sswebpack_engine_only": "dev-master"
  },
  "scripts": {
    "post-install-cmd": [],
    "post-update-cmd": []
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "keywords": [
    "Silverstripe-CMS",
    "CMS",
    "Silverstripe",
    "ecommerce",
    "e-commerce",
    "shop",
    "sales",
    "demo"
  ],
  "extra": {
    "installer-name": "ecommerce_test",
    "project-files-installed": [
      "app/.htaccess",
      "app/_config.php",
      "app/_config/mimevalidator.yml",
      "app/_config/mysite.yml",
      "app/src/Page.php",
      "app/src/PageController.php"
    ],
    "public-files-installed": [
      ".htaccess",
      "index.php",
      "web.config"
    ]
  },
  "license": "BSD-3-Clause",
  "autoload": {
    "psr-4": {
      "Sunnysideup\\EcommerceTest\\": "app/src/"
    },
    "files": [
      "app/src/Page.php",
      "app/src/PageController.php"
    ]
  },
  "config": {
    "sort-packages": true
  }
}
